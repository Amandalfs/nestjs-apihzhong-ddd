// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum RulesAuth {
  member
  adminCustomer
  adminSuport
  adminWallet
}

model AuthUser {
  id       String    @id
  email    String    @unique
  password String
  rules    RulesAuth @default(member)
}
